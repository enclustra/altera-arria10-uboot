altera_set_storage QSPI;

sf probe || exit;

sf read ${ramdisk_loadaddr} ${qspi_ramdisk_offset} ${ramdisk_size} || exit;
sf read ${kernel_loadaddr} ${qspi_kernel_offset} ${kernel_size} || exit;
sf read ${devicetree_loadaddr} ${qspi_devicetree_offset} ${devicetree_size} || exit;

run ramdiskargs;

bootm ${kernel_loadaddr} ${ramdisk_loadaddr} ${devicetree_loadaddr};
